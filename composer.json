{
    "name": "sweeper/helper-php",
    "description": "helper tool、function",
    "type": "library",
    "keywords": [
        "PHP",
        "Helpers",
        "Tools",
        "Components"
    ],
    "homepage": "https://github.com/HypnosKiss/helper-php/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "sweeper",
            "email": "wili.lixiang@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-memcache": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-zip": "*",
        "elasticsearch/elasticsearch": "^7.11.0 || ^v8.0.0",
        "guzzlehttp/guzzle": "^7.5.0",
        "monolog/monolog": "^2.9.1 || ^3.0.0",
        "php-amqplib/php-amqplib": "^3.3.0",
        "predis/predis": "^2.2.0 || ^v3",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "sweeper/design-pattern": "^1.0 || ^2.0",
        "sweeper/guzzlehttp-request": "^1.0 || ^2.0",
        "symfony/process": "^5.4.28"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/var-dumper": "^5.4.29",
        "symfony/console": "^5.4.31"
    },
    "autoload": {
        "psr-4": {
            "Sweeper\\HelperPhp\\": "src/"
        },
        "files": [
            "src/helper.php",
            "src/Func/array.php",
            "src/Func/curl.php",
            "src/Func/env.php",
            "src/Func/file.php",
            "src/Func/string.php",
            "src/Func/time.php",
            "src/Func/util.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sweeper\\HelperPhp\\Test\\": "test"
        }
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
